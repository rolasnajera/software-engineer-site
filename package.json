{
  "name": "rolasnajera-site",
  "version": "0.1.0",
  "description": "Rolas Najera personal site",
  "source": "src/index.html",
  "scripts": {
    "dev": "parcel src/index.html",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build": "parcel build",
    "start": "parcel",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RolandoNajera/rolasnajera-site.git"
  },
  "keywords": [
    "rolasnajera",
    "personal-site",
    "react"
  ],
  "author": "Rolas Najera",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RolandoNajera/rolasnajera-site/issues"
  },
  "homepage": "https://rolasnajera.github.io/rolasnajera.dev",
  "devDependencies": {
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "gh-pages": "^4.0.0",
    "parcel": "^2.5.0",
    "prettier": "^2.6.2",
    "process": "^0.11.10"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
